{
    "template" : "filebeat-*",
    "mappings": {
        "accesslog": {
            "properties": {
                "timestamp": {
                    "type": "date",
                    "index" : "not_analyzed"
                },
                "ip_address": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "method": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "uri_path": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "source": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "type": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "input_type": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "uri_path_params": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "ecs-customer": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "ecs-node-ip": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
               "ecs-node-name": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "ecs-vdc-id": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "ecs-vdc-name": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "response_code" : {
                    "type": "integer"
                },
                "ecs-storagepool-name": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "ecs-storagepool-id": {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "client_host" : {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "verb" : {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "server_host" : {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "server_port" : {
                    "type": "string",
                    "index" : "not_analyzed"
                },
                "is_error" : {
                    "type": "integer"
                },
                "is_success" : {
                    "type": "integer"
                },
                "is_user_error" : {
                    "type": "integer"
                },
                "is_system_error" : {
                    "type": "integer"
                },
                "is_user_error_bool" : {
                    "type": "integer"
                },
                "is_system_error_bool" : {
                    "type": "integer"
                },
                "duration" : {
                    "type": "float"
                },
                "upload" : {
                    "type": "float"
                },
                "download" : {
                    "type": "float"
                }
            }
        }
    }
}
